{
  "name": "turtle-script-lang",
  "displayName": "turtle-script",
  "description": "Syntax highlighting support for TurtleScript by Mason Tuttle",
  "publisher": "masontuttle",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skiffy106/turtle-script-lang.git"
  },
  "license": "MIT",
  "icon": "images/icon.png",
  "keywords": [
    "vscode",
    "syntax-highlighting",
    "turtle-script",
    "turtleScript"
  ],
  "contributes": {
    "languages": [
      {
        "id": "turtle",
        "aliases": [
          "TurtleScript",
          "turtle"
        ],
        "extensions": [
          ".trtl",
          ".turtle"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "turtle",
        "scopeName": "source.turtle-script",
        "path": "./syntaxes/turtle.tmLanguage.json"
      }
    ]
  }
}